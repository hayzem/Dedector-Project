;reads and print contents of a file 
;------------------------------------------------------------------

pro readfw,fname

;------------------------------------------------------------------
;Yiğit Dallılar 27.08.2012
;------------------------------------------------------------------
  
  openr,1,fname
  t=''

  while ~ eof(1) do begin
     readf,1,t
     print,t
  endwhile

  close,1

end
